{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 12",
	"compilerOptions": {
		"lib": [
			"DOM",
			"ES2015",
			"ES2016",
			"ES2017",
			"ES2018"
		],
		"module": "commonjs",
		"target": "ES2018",
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"main.ts"
	]
}